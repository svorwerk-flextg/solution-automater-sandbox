{
  "dashboard": {
    "id": null,
    "title": "Solution-Automater-Sandbox Overview",
    "description": "Comprehensive overview of SAS platform health, performance, and utilization",
    "tags": ["solution-automater-sandbox", "overview", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"sas-.*\"}",
            "refId": "A",
            "legendFormat": "{{job}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Agent Pool Status",
        "type": "stat", 
        "targets": [
          {
            "expr": "sas_active_agents_total",
            "refId": "A",
            "legendFormat": "Active Agents"
          },
          {
            "expr": "sas_agent_capacity_total",
            "refId": "B", 
            "legendFormat": "Total Capacity"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"sas-.*\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "{{name}} CPU %"
          },
          {
            "expr": "container_memory_usage_bytes{name=~\"sas-.*\"} / container_spec_memory_limit_bytes{name=~\"sas-.*\"} * 100",
            "refId": "B",
            "legendFormat": "{{name}} Memory %"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "xAxis": {"show": true},
        "yAxes": [
          {
            "label": "Percentage",
            "max": 100,
            "min": 0,
            "show": true,
            "unit": "percent"
          }
        ],
        "legend": {
          "avg": true,
          "current": true,
          "max": true,
          "min": true,
          "show": true,
          "total": false,
          "values": true
        },
        "nullPointMode": "null as zero",
        "steppedLine": false,
        "tooltip": {"shared": true, "sort": 0, "value_type": "individual"}
      },
      {
        "id": 4,
        "title": "Database Proxy Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sas_db_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{database}} Requests/sec"
          },
          {
            "expr": "sas_db_connection_pool_active",
            "refId": "B",
            "legendFormat": "{{database}} Active Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "yAxes": [
          {
            "label": "Count",
            "show": true,
            "unit": "short"
          }
        ],
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 5,
        "title": "Security Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sas_security_events_total[5m])",
            "refId": "A",
            "legendFormat": "{{severity}} Events/sec"
          },
          {
            "expr": "sas_blocked_requests_total",
            "refId": "B",
            "legendFormat": "Blocked Requests"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "yAxes": [
          {
            "label": "Count",
            "show": true,
            "unit": "short"
          }
        ],
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 6,
        "title": "Cloud Integration Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sas_cloud_connector_status{service=\"fabric\"}",
            "refId": "A",
            "legendFormat": "Microsoft Fabric"
          },
          {
            "expr": "sas_cloud_connector_status{service=\"aws\"}",
            "refId": "B",
            "legendFormat": "AWS"
          },
          {
            "expr": "sas_cloud_connector_status{service=\"azure\"}",
            "refId": "C",
            "legendFormat": "Azure"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 24},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DISCONNECTED", "color": "red"},
                  "1": {"text": "CONNECTED", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "MCP Servers Health",
        "type": "stat",
        "targets": [
          {
            "expr": "sas_mcp_server_status",
            "refId": "A",
            "legendFormat": "{{server_name}}"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 24},
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "UNHEALTHY", "color": "red"},
                  "1": {"text": "HEALTHY", "color": "green"},
                  "2": {"text": "WARNING", "color": "yellow"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Recent Alerts",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"sas-audit-logger\", level=\"error\"}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 24},
        "options": {
          "showCommonLabels": false,
          "showLabels": false,
          "showTime": true,
          "sortOrder": "Descending",
          "wrapLogMessage": true
        }
      },
      {
        "id": 9,
        "title": "Task Execution Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sas_tasks_completed_total[5m])",
            "refId": "A",
            "legendFormat": "Tasks Completed/sec"
          },
          {
            "expr": "rate(sas_tasks_failed_total[5m])",
            "refId": "B", 
            "legendFormat": "Tasks Failed/sec"
          },
          {
            "expr": "sas_task_queue_length",
            "refId": "C",
            "legendFormat": "Queue Length"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "yAxes": [
          {
            "label": "Count",
            "show": true,
            "unit": "short"
          }
        ],
        "legend": {"show": true, "values": true, "current": true, "avg": true},
        "tooltip": {"shared": true}
      },
      {
        "id": 10,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(sas_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(sas_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(sas_request_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "yAxes": [
          {
            "label": "Duration",
            "show": true,
            "unit": "s"
          }
        ],
        "legend": {"show": true, "values": true, "current": true},
        "tooltip": {"shared": true}
      }
    ],
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(up{job=~\"sas-.*\"}, job)",
          "hide": 0,
          "includeAll": true,
          "label": "Service",
          "multi": true,
          "name": "service",
          "options": [],
          "query": "label_values(up{job=~\"sas-.*\"}, job)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "allValue": null,
          "current": {
            "selected": true,
            "text": "5m",
            "value": "5m"
          },
          "hide": 0,
          "includeAll": false,
          "label": "Time Range",
          "multi": false,
          "name": "range",
          "options": [
            {"selected": false, "text": "1m", "value": "1m"},
            {"selected": true, "text": "5m", "value": "5m"},
            {"selected": false, "text": "15m", "value": "15m"},
            {"selected": false, "text": "1h", "value": "1h"}
          ],
          "query": "1m,5m,15m,1h",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(sas_deployment_info[1h]) > 0",
          "iconColor": "green",
          "name": "Deployments",
          "titleFormat": "Deployment",
          "tagKeys": "version,service"
        }
      ]
    },
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "targetBlank": true,
        "title": "SAS Documentation",
        "tooltip": "Solution-Automater-Sandbox Documentation",
        "type": "link",
        "url": "https://docs.solution-automater-sandbox.com"
      },
      {
        "icon": "external link",
        "tags": [],
        "targetBlank": true,
        "title": "SAS GitHub",
        "tooltip": "Solution-Automater-Sandbox GitHub Repository",
        "type": "link",
        "url": "https://github.com/solution-automater-sandbox/claude-docker"
      }
    ]
  }
}